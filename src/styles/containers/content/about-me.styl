.about-me
  min-height 537px
  background-color #f3f3f3
  padding-top 50px

  h2
    padding 45px 0 20px

  p
    font-size: 1.09rem
    font-family: cabinregular
    color: rgb(83, 83, 83)
    text-align: center

.my-photo
  width 176px
  height 176px
  background-color white
  margin auto
  img
    width 100%
    height 100%
    border-radius 100%

.location
  position relative
  padding-top 90px

  &:before
    content ""
    position absolute
    height 21px
    width 18px
    top: 50px
    left calc(50% - 9px)
    background url("/img/Located_icon1.png")
